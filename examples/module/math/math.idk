// package math

func sqrt(val:float) -> float
	// Initialize the result to a guess
	result := val / 2.0

	// Continue iterating until the result is accurate to within 0.00001
    i := 0
	for i < 10
		result = result - (result * result - val) / (2.0 * result)
        i = i + 1
	end

	return result
end
